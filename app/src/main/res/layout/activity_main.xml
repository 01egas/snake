    <Button
            android:text="Play"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnPlay" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:onClick="onClickPlay" tools:layout_editor_absoluteX="200dp"/>
    <Button
            android:text="UP"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnUp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/glGameSpace" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:onClick="onClickChangeDirection"/>
    <Button
            android:text="left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnLeft"
            app:layout_constraintTop_toBottomOf="@+id/btnUp" app:layout_constraintEnd_toStartOf="@+id/btnRight"
            android:layout_marginEnd="88dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.97"
            android:onClick="onClickChangeDirection"/>
    <Button
            android:text="right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnRight"
            app:layout_constraintTop_toBottomOf="@+id/btnUp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="72dp" android:onClick="onClickChangeDirection"/>
    <Button
            android:text="down"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnDown"
            app:layout_constraintTop_toBottomOf="@+id/btnLeft" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:onClick="onClickChangeDirection" app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:text="stop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnStop" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/btnPlay"
            android:layout_marginStart="8dp" android:onClick="onClickStop"/>
</android.support.constraint.ConstraintLayout>